0004C5A2                            ; Object Positioning
0004C5A2                            ; ---------------------------------------------------------------------------
0004C5A2                            ;
0004C5A2                            ; Format:	dc.w	$XXXX,$YYYY,$AAAA,$FFFF
0004C5A2                            ;
0004C5A2                            ; XXXX 	=	Object's X Position (If FFFF is placed in X position then it's the end of the Object position file)
0004C5A2                            ; YYYY	=	Object's Y Position
0004C5A2                            ; AAAA	=	Object's ID
0004C5A2                            ; FFFF	=	End of Object's position code (Unless the object is a pathswapper)
0004C5A2                            ;
0004C5A2                            ; ---------------------------------------------------------------------------
0004C5A2                            ; List of Valid and Invalid Object ID's
0004C5A2                            ;
0004C5A2                            ; AAAA = changes exactly what object is being placed (This cannot be an odd value or the engine will crash)
0004C5A2                            ;
0004C5A2 =00000000                  SpringRed_Right		=	$00
0004C5A2 =00000004                  SpringRed_Left		=	$04
0004C5A2 =00000008                  SpringRed_Up		=	$08
0004C5A2 =0000000C                  SpringRed_Down		=	$0C
0004C5A2 =00000010                  Goal				=	$10
0004C5A2 =00000014                  DiagRedSprngRU		=	$14
0004C5A2 =00000018                  DiagRedSprngLU		=	$18
0004C5A2 =0000001C                  DiagRedSprngRD		=	$1C
0004C5A2 =00000020                  DiagRedSprngLD		=	$20
0004C5A2                            ;					=	$24
0004C5A2 =00000028                  LostRing			=	$28
0004C5A2                            ;					=	$2C
0004C5A2                            ;					=	$30
0004C5A2 =00000034                  SpringYel_Right		=	$34
0004C5A2 =00000038                  SpringYel_Left		=	$38
0004C5A2 =0000003C                  SpringYel_Up		=	$3C
0004C5A2 =00000040                  SpringYel_Down		=	$40
0004C5A2 =00000044                  DiagYelSprngRU		=	$44
0004C5A2 =00000048                  DiagYelSprngLU		=	$48
0004C5A2 =0000004C                  DiagYelSprngRD		=	$4C
0004C5A2 =00000050                  DiagYelSprngLD		=	$50
0004C5A2 =00000054                  Spikes_Up			=	$54
0004C5A2 =00000058                  Spikes_Down			=	$58
0004C5A2 =0000005C                  Spikes_Right		=	$5C
0004C5A2 =00000060                  Spikes_Left			=	$60
